---
import Layout from "@/layouts/Layout.astro";
---

<Layout title="ACRECONRE">
  <div class="flex flex-col items-center text-center mt-10">
    
    <p class="text-2xl md:text-3xl font-bold uppercase">
      Página en construcción
    </p>

    <p class="mt-3 text-lg md:text-xl font-semibold text-gray-600">
      INSCRIPCIONES SE ABRIRAN PRONTO
    </p>

    <img
      src="/images/acreconre/acreconre_logo_full.webp"
      draggable="false"
      alt="ACRECONRE Banner"
      class="mt-8 w-full max-w-lg"
    />
    
  </div>
</Layout>
